{
  "name": "vue-selectize",
  "version": "1.0.3",
  "description": "A Vue.js directive for selectize.js",
  "repository": "michaelowens/vue-selectize",
  "bugs": {
    "url": "https://github.com/michaelowens/vue-selectize/issues"
  },
  "main": "./index.js",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dev/build.js",
    "serve": "http-server -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -e src/main.js | uglifyjs -c warnings=false -m > dev/build.js",
    "lint": "eslint --ext .js,.vue src test/unit",
    "test": "karma start karma.conf.js"
  },
  "keywords": [
    "vue",
    "vuejs",
    "selectize",
    "dropdown",
    "select"
  ],
  "author": "Michael Owens",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.0.0",
    "selectize": "^0.12.1",
    "vue": "^1.0.16"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "bootstrap-sass": "^3.0.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.5",
    "eslint": "^1.10.3",
    "eslint-plugin-html": "^1.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.23",
    "moment": "^2.12.0",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.6.0",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^1.2.2",
    "vueify": "^8.0.0",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "homepage": "https://github.com/michaelowens/vue-selectize#readme",
  "directories": {
    "test": "test"
  }
}
